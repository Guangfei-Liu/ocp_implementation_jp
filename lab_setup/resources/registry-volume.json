{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "registry-storage"
  },
  "spec": {
    "capacity": {
        "storage": "15Gi"
        },
    "accessModes": [ "ReadWriteMany" ],
    "nfs": {
        "path": "/var/export/pvs/registry-storage",
        "server": "oselab-${GUID}.oslab.opentlc.com"
    }
  }
}
